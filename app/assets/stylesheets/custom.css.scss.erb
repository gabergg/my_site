#navcontainer ul {
    list-style-type: none;
    margin: 30px 0px 0px 20px;
    padding: 0;
}

#navcontainer ul li {
    display: inline;
    margin: 0 30px;
    padding-bottom: 10px;
}

#navcontainer ul li a {
    font-weight: bold;
    font-size: 24px;
    color: #FFFAF0;
}


.activeNav {
    border-bottom: 3px solid #4F2F2F;
}

.activeNav a {
    color: #4F2F2F !important;
}